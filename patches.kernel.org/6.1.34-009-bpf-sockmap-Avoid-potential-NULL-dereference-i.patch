diff --git a/net/core/skmsg.c b/net/core/skmsg.c
index 9e0f69451563..65fb6f5b21b2 100644
--- a/net/core/skmsg.c
+++ b/net/core/skmsg.c
@@ -1205,7 +1205,8 @@ static void sk_psock_verdict_data_ready(struct sock *sk)
 
 		rcu_read_lock();
 		psock = sk_psock(sk);
-		psock->saved_data_ready(sk);
+		if (psock)
+			psock->saved_data_ready(sk);
 		rcu_read_unlock();
 	}
 }
