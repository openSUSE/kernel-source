--- linux-2.6.32-rc3/drivers/scsi/bfa/include/bfi/bfi_fcpim.h	1970-01-01 00:00:00.000000000 +0000
+++ linux-2.6.32-rc4/drivers/scsi/bfa/include/bfi/bfi_fcpim.h	2009-10-11 22:05:45.146781208 +0000
@@ -0,0 +1,301 @@
