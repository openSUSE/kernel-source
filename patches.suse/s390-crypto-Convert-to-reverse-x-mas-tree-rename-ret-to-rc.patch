-	if (ret)
-		return ret;
+	rc = __paes_keyblob2pkey(&ctx->kb, &pkey);
+	if (rc)
+		return rc;
 
 	spin_lock_bh(&ctx->pk_lock);
 	memcpy(&ctx->pk, &pkey, sizeof(pkey));
