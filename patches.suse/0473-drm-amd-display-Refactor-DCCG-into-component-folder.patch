-	dccg31_disable_symclk32_se(dccg, 0);
-	dccg31_disable_symclk32_se(dccg, 1);
-	dccg31_disable_symclk32_se(dccg, 2);
-	dccg31_disable_symclk32_se(dccg, 3);
