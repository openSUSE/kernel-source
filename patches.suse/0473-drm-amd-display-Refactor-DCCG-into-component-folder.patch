-{
-	struct dcn_dccg *dccg_dcn = TO_DCN_DCCG(dccg);
-
-	REG_UPDATE(DISPCLK_FREQ_CHANGE_CNTL,
-			DCCG_FIFO_ERRDET_OVR_EN, en ? 1 : 0);
-}
