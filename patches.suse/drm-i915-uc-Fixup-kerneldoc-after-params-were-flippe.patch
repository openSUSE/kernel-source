+++ b/drivers/gpu/drm/i915/gt/uc/intel_uc_fw.c
@@ -185,8 +185,8 @@ void intel_uc_fw_init_early(struct intel_uc_fw *uc_fw,
 /**
  * intel_uc_fw_fetch - fetch uC firmware
  *
- * @dev_priv: device private
  * @uc_fw: uC firmware
