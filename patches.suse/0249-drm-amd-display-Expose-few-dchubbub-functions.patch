 		unsigned int refclk_mhz,
@@ -713,7 +713,7 @@ static bool hubbub32_program_usr_watermarks(
 	return wm_pending;
 }
 
-static void hubbub32_force_usr_retraining_allow(struct hubbub *hubbub, bool allow)
