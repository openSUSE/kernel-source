+++ b/drivers/gpu/drm/nouveau/nvkm/engine/device/base.c
@@ -2395,7 +2395,7 @@ nv13b_chipset = {
 	.mc = gp10b_mc_new,
 	.mmu = gp10b_mmu_new,
 	.secboot = gp10b_secboot_new,
-	.pmu = gm20b_pmu_new,
+	.pmu = gp10b_pmu_new,
 	.timer = gk20a_timer_new,
 	.top = gk104_top_new,
 	.ce[0] = gp100_ce_new,
