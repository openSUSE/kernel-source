--- linux-2.6.11-rc2-bk10/arch/mips/kernel/signal-common.h	1970-01-01 01:00:00.000000000 +0100
+++ linux-2.6.11-rc3/arch/mips/kernel/signal-common.h	2005-02-03 02:55:53.000000000 +0100
@@ -0,0 +1,137 @@
